<h1>Editing Artist #<%= @artist.id %></h1>

<form action="/artists/<%= @artist.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @artist.name  %>" type="text" />
  </div>
  <div>
    <label for="description">description</label><br />
    <input id="description" name="description" value="<%= @artist.description  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


